<section class="servicebox p-lg-0 pb-0 pq-bg-grey ">
    <div class="container pq-mt-60">
        <div class="row justify-content-center">
            <div class="col-lg-6 col-md-6 col-12 pt-md-0 pt-4 mb-4" data-aos-delay="100" data-aos-duration="800" data-aos="zoom-in-down">
                <div class="pq-info-box pq-style-2">
                    <div class="pq-info-box-right-icon"> <i class=" flaticon-medical-doctor"></i> </div>
                    <div class="pq-info-box-icon"><i class="fa-solid fa-disease"></i></div>
                    <h5 class="pq-info-title">@Localizer["TitleService_2"]</h5>
                    <p class="pq-infobox-description">
                        @Localizer["ContentService_2"]
                    </p>
                    
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-12 pt-xl-0 pt-4" data-aos-delay="100" data-aos-duration="800" data-aos="zoom-in-down">
                <div class="pq-info-box pq-style-3">
                    <div class="pq-info-box-right-icon"> <i class=" flaticon-care"></i> </div>
                    <div class="pq-info-box-icon"><i class="fa-solid fa-seedling"></i></div>
                    <h5 class="pq-info-title">@Localizer["TitleService_4"]</h5>
                    <p class="pq-infobox-description">
                        @Localizer["ContentService_4"]
